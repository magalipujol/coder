<div class="container mt-5">
  <div class="row">
    <div class="col">
      <table class="table">
        <thead class="table">
          <th>ID</th>
          <th>Nombre</th>
          <th>Precio</th>
          <th>Imagen</th>
        </thead>
        <tbody>
          {{#each productos}}
            <tr>
              <td>{{this.id}}</td>
              <td>{{this.nombre}}</td>
              <td>{{this.precio}}</td>
              <td><img src="{{this.thumbnail}}" height="30px" /></td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>